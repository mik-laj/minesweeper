ome/zuzanna/Pulpit/saper/src/pages/Minesweeper.tsx",["154"],"import React, { useState, useEffect } from 'react'\nimport { useSelector } from 'react-redux'\n\nimport Board from '../minesweeper/board/Board'\nimport Timer from './../components/Timer'\nimport { IoMdFlag } from 'react-icons/io'\nimport { BiSmile, BiSad } from 'react-icons/bi'\nimport { Wrapper, Menu } from './Minesweeper.css'\nimport Modal from './../components/modal/Modal'\nimport { RootState } from './../reducers/index'\nimport WinModal from './../components/winModal/WinModal'\n\nconst Minesweeper = () => {\n  const selectorParameters = useSelector((state: RootState) => state.gameParameters)\n\n  const [width, setWidth] = useState(selectorParameters.width)\n  const [height, setHeight] = useState(selectorParameters.height)\n  const [mines, setMines] = useState(selectorParameters.mines)\n  const [isLose, setIsLose] = useState(false)\n  const [isWin, setIsWin] = useState(false)\n  const [winTime, setWinTime] = useState(0)\n  const [isRefresh, setIsRefresh] = useState(false)\n  const [flags, setFlags] = useState(selectorParameters.mines)\n  const [isOpenModal, setIsOpenModal] = useState(false)\n\n  useEffect(() => {\n    if (isWin) {\n      setTimeout(() => {\n        setIsOpenModal(true)\n      }, 500);\n    }\n  }, [isWin]);\n\n  useEffect(() => {\n    setWidth(selectorParameters.width)\n    setHeight(selectorParameters.height)\n    setMines(selectorParameters.mines)\n    restart();\n  }, [selectorParameters]);\n\n  const restart = () => {\n    setIsLose(false)\n    setFlags(selectorParameters.mines)\n    setIsRefresh(true)\n    setIsWin(false)\n\n    setTimeout(() => {\n      setIsRefresh(false)\n    }, 10);\n  }\n\n  return (\n    <Wrapper>\n      <Modal startOpen={isOpenModal} close={() => setIsOpenModal(false)}>\n        <WinModal\n          time={winTime}\n          width={width}\n          height={height}\n          mines={mines}\n          level={selectorParameters.level}\n          closeModal={() => setIsOpenModal(false)}\n        />\n      </Modal>\n      <Menu>\n        <IoMdFlag />\n        <span style={{ fontSize: 20, marginRight: 30 }}>\n          {flags}\n        </span>\n        {\n          isLose ?\n            (<BiSad style={{ cursor: 'pointer' }} onClick={restart} />) :\n            (<BiSmile style={{ cursor: 'pointer' }} onClick={restart} />)\n        }\n        <Timer\n          isRefresh={isRefresh}\n          isWin={isWin}\n          setWinTime={setWinTime}\n          isLose={isLose}\n        />\n      </Menu>\n      {\n        !isRefresh ?\n          <Board\n            width={width}\n            height={height}\n            mines={mines}\n            setFlags={setFlags}\n            setIsLose={setIsLose}\n            isLose={isLose}\n            setIsWin={setIsWin}\n            isWin={isWin}\n          /> : null\n      }\n    </Wrapper>\n  )\n}\n\nexport default Minesweeper;","/home/zuzanna/Pulpit/saper/src/pages/MainPage.tsx",[],"/home/zuzanna/Pulpit/saper/src/pages/Score.tsx",["155","156","157"],"/home/zuzanna/Pulpit/saper/src/minesweeper/board/Board.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/board/Board.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/board/boardGenerator.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/field/Field.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/field/Field.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/field/Marks.tsx",[],"/home/zuzanna/Pulpit/saper/src/pages/Minesweeper.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/Timer.tsx",["158","159"],"import React, { useState, useEffect, FunctionComponent, useRef } from 'react'\nimport { FiClock } from 'react-icons/fi'\n\ntype TimerProps = {\n  isRefresh: boolean\n  isWin: boolean\n  setWinTime: (time: number) => void\n  isLose: boolean\n}\n\nconst Timer: FunctionComponent<TimerProps> = ({ isRefresh, isWin, setWinTime, isLose }) => {\n  let [time, setTime] = useState(0);\n\n  const intervalID = useRef(0);\n\n  const startInterval = () => {\n    intervalID.current = setInterval(() => {\n      time++\n      setTime(time)\n      if (time === 999) {\n        clearInterval(intervalID.current)\n      }\n    }, 1000);\n  }\n\n  const stopInterval = () => {\n    clearInterval(intervalID.current);\n    setTime(0)\n  }\n\n  useEffect(() => {\n    return () => stopInterval();\n  }, []);\n\n  useEffect(() => {\n    if (isRefresh) {\n      stopInterval();\n    } else {\n      startInterval()\n    }\n  }, [isRefresh]);\n\n  useEffect(() => {\n    if (isWin) {\n      clearInterval(intervalID.current);\n      setWinTime(time)\n    }\n    if (isLose) {\n      clearInterval(intervalID.current);\n    }\n  }, [isWin, isLose]);\n\n  return (\n    <>\n      <FiClock style={{ marginLeft: 30 }} />\n      <span style={{ fontSize: 20, margin: 5 }}>\n        {(\"000\" + time).slice(-3)}\n      </span>\n    </>\n  )\n}\n\nexport default Timer;","/home/zuzanna/Pulpit/saper/src/components/modal/Modal.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/modal/Modal.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/index.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/gameParameters/reducers.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/gameParameters/types.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/gameTheme/reducers.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/gameTheme/types.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/winModal/WinModal.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/winModal/WinModal.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/pages/MainPage.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/pages/Score.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/score/SingleScore.tsx",["160"],"import React, { FunctionComponent } from 'react'\nimport { SingleWinner, Row, Index, ColumnHead } from './SingleScore.css'\n\nexport interface Score {\n  name: string,\n  time: number,\n  id: number\n}\n\nconst Score: FunctionComponent<Score> = ({ name, time, id }) => {\n  return (\n    <SingleWinner>\n      <Row>\n        <Index>{id}</Index>\n        <ColumnHead style={{width: '15%'}}>name:</ColumnHead>\n        <div style={{width: '60%'}}>{name}</div>\n        <ColumnHead style={{width: '15%'}}>time:</ColumnHead>\n        <div style={{width: '10%'}}>{time}</div>\n      </Row>\n    </SingleWinner>\n  )\n}\n\nexport default Score;","/home/zuzanna/Pulpit/saper/src/score/SingleScore.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/settings/Settings.tsx",["161","162"],"import React, { useState } from 'react'\nimport { GearButton, SubmitButton, Wrapper } from './Settings.css'\nimport Modal from '../modal/Modal'\nimport Level from './Level'\nimport { Levels } from '../../../reducers/gameParameters/types'\nimport { SetParametersAcition } from '../../../reducers/gameParameters/actions'\nimport { RootState } from '../../../reducers/index'\nimport { useSelector, useDispatch } from 'react-redux'\n\nconst Settings = () => {\n  const selectorParamaters = useSelector((state: RootState) => state.gameParameters)\n\n  const dispatch = useDispatch();\n  const [parameters, setParameters] = useState(selectorParamaters)\n  const [isOpen, setIsOpen] = useState(false);\n\n  const handleChangeParameters = (event: React.ChangeEvent<HTMLInputElement>) => {\n    const { name, value } = event.target;\n    if ((name === 'width' || name === 'height') && parameters.mines >= (parameters.width - 1) * (parameters.height - 1)) {\n      const newMines = name === 'width' ? (value as any - 1) * (parameters.height - 1) : (parameters.width - 1) * (value as any - 1);\n      setParameters((prevState: any) => ({\n        ...prevState,\n        [name]: value,\n        ['mines']: newMines,\n      }))\n    } else {\n      setParameters((prevState: any) => ({\n        ...prevState,\n        [name]: value\n      }))\n    }\n  }\n\n  const handleChangeThemes = (event: React.ChangeEvent<HTMLInputElement>) => {\n    const { value } = event.target;\n    setThemes({ theme: parseInt(value) })\n  }\n\n  const sa