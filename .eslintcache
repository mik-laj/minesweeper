, [isWin]);\n\n  useEffect(() => {\n    setWidth(selectorParameters.width)\n    setHeight(selectorParameters.height)\n    setMines(selectorParameters.mines)\n    restart();\n  }, [selectorParameters]);\n\n  const restart = () => {\n    setIsLose(false)\n    setFlags(selectorParameters.mines)\n    setIsRefresh(true)\n    setIsWin(false)\n\n    setTimeout(() => {\n      setIsRefresh(false)\n    }, 10);\n  }\n\n  return (\n    <Wrapper>\n      <Modal startOpen={isOpenModal} close={() => setIsOpenModal(false)}>\n        <WinModal\n          time={winTime}\n          width={width}\n          height={height}\n          mines={mines}\n          level={selectorParameters.level}\n          closeModal={() => setIsOpenModal(false)}\n        />\n      </Modal>\n      <Menu>\n        <IoMdFlag />\n        <span style={{ fontSize: 20, marginRight: 30 }}>\n          {flags}\n        </span>\n        {\n          isLose ?\n            (<BiSad style={{ cursor: 'pointer' }} onClick={restart} />) :\n            (<BiSmile style={{ cursor: 'pointer' }} onClick={restart} />)\n        }\n        <Timer\n          isRefresh={isRefresh}\n          isWin={isWin}\n          setWinTime={setWinTime}\n          isLose={isLose}\n        />\n      </Menu>\n      {\n        !isRefresh ?\n          <Board\n            width={width}\n            height={height}\n            mines={mines}\n            setFlags={setFlags}\n            setIsLose={setIsLose}\n            isLose={isLose}\n            setIsWin={setIsWin}\n            isWin={isWin}\n          /> : null\n      }\n    </Wrapper>\n  )\n}\n\nexport default Minesweeper;","/home/zuzanna/Pulpit/saper/src/pages/MainPage.tsx",[],"/home/zuzanna/Pulpit/saper/src/pages/Score.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/board/Board.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/board/Board.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/board/boardGenerator.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/field/Field.tsx",[],"/hom