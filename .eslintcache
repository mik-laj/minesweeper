   setTheme(selectorThemes.theme)\n    restart();\n  }, [selectorParameters, selectorThemes]);\n\n  const restart = () => {\n    setIsLose(false)\n    setFlags(selectorParameters.mines)\n    setIsRefresh(true)\n    setIsWin(false)\n\n    setTimeout(() => {\n      setIsRefresh(false)\n    }, 10);\n  }\n\n  return (\n    <Wrapper>\n      <Modal startOpen={isOpenModal} close={() => setIsOpenModal(false)}>\n        <WinModal\n          time={winTime}\n          width={width}\n          height={height}\n          mines={mines}\n          level={selectorParameters.level}\n          closeModal={() => setIsOpenModal(false)}\n        />\n      </Modal>\n      <Menu>\n        <IoMdFlag />\n        <span style={{ fontSize: 20, marginRight: 30 }}>\n          {flags}\n        </span>\n        {\n          isLose ?\n            (<BiSad style={{ cursor: 'pointer' }} onClick={restart} />) :\n            (<BiSmile style={{ cursor: 'pointer' }} onClick={restart} />)\n        }\n        <Timer\n          isRefresh={isRefresh}\n          isWin={isWin}\n          setWinTime={setWinTime}\n          isLose={isLose}\n        />\n      </Menu>\n      {\n        !isRefresh ?\n          <Board\n            width={width}\n            height={height}\n            mines={mines}\n            setFlags={setFlags}\n            setIsLose={setIsLose}\n            isLose={isLose}\n            setIsWin={setIsWin}\n            theme={theme}\n          /> : null\n      }\n    </Wrapper>\n  )\n}\n\nexport default Minesweeper;","/home/zuzanna/Pulpit/saper/src/pages/MainPage.tsx",[],"/home/zuzanna/Pulpit/saper/src/pages/Score.tsx",["160","161","162"],"import React, { useEffect, useState } from 'react'\nimport axios from 'axios'\nimport { Wrapper } from './Score.css'\nimport SingleScore from './../score/SingleScore'\nimport TableTitle from './../score/TableTitle'\n\nexport interface Winner {\n  name: string,\n  time: number,\n  width: number,\n  height: number,\n  mines: number,\n  id: number,\n  level: number\n}\n\nexport interface Winners {\n  winners: Winner[][]\n}\n\nconst Score = () => {\n  const [winners, setWinners] = useState<Winners>({ winners: [] })\n\n  const sortByTime = (a: Winner, b: Winner): number => {\n    return a.time - b.time\n  }\n\n  const sortCustom = (a: Winner, b: Winner): number => {\n    if (a.width < b.width || a.height < b.height || a.mines < b.mines) {\n      return 1;\n    }\n    if (a.width > b.width || a.height > b.height || a.mines > b.mines) {\n      return -1;\n    }\n\n    return 0;\n  }\n\n  useEffect(() => {\n    async function getWinnerApi() {\n      try {\n        const resp = await axios.get('http://localhost:3002/winners')\n\n        for (let i = 0; i < 3; i++) {\n\n          const w: Winner[] = resp.data.filter((value: any) => {\n            return value.level === 2 - i;\n          }).sort((a: Winner, b: Winner) => sortByTime(a, b))\n\n          if (w.length > 0) {\n            winners.winners.push(w);\n          }\n        }\n\n        let wCustom: Winner[] = resp.data.filter((value: Winner) => {\n          return value.level === 3;\n        }).sort((a: Winner, b: Winner) => sortCustom(a, b))\n\n        while (wCustom.length > 0) {\n          winners.winners.push(wCustom.filter((value: Winner) => {\n            return value.height === wCustom[0].height && value.width === wCustom[0].width\n              && value.mines === wCustom[0].mines\n          }).sort((a: Winner, b: Winner) => sortByTime(a, b)))\n\n          wCustom = wCustom.filter((value: Winner) => {\n            return !(value.height === wCustom[0].height && value.width === wCustom[0].width\n              && value.mines === wCustom[0].mines)\n          })\n        }\n\n        setWinners({ winners: winners.winners.slice() })\n      } catch (err) {\n        console.log(err)\n      }\n    }\n    getWinnerApi()\n  }, []);\n\n  return (\n    <Wrapper>\n      <h1>Score</h1>\n      {winners.winners.map((row) => (\n        <>\n          <TableTitle\n            level={row[0].level}\n            width={row[0].width}\n            height={row[0].height}\n            mines={row[0].mines}\n          />\n          {row.map((el, index2) => (\n            <SingleScore\n              id={index2 + 1}\n              name={el.name}\n              time={el.time}\n            />\n          ))}\n        </>\n      ))}\n    </Wrapper>\n  )\n}\n\nexport default Score","/home/zuzanna/Pulpit/saper/src/minesweeper/board/Board.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/board/Board.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/board/boardGenerator.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/field/Field.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/field/Field.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/minesweeper/field/Marks.tsx",[],"/home/zuzanna/Pulpit/saper/src/pages/Minesweeper.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/Timer.tsx",["163","164"],"import React, { useState, useEffect, FunctionComponent, useRef } from 'react'\nimport { FiClock } from 'react-icons/fi'\n\ntype TimerProps = {\n  isRefresh: boolean\n  isWin: boolean\n  setWinTime: (time: number) => void\n  isLose: boolean\n}\n\nconst Timer: FunctionComponent<TimerProps> = ({ isRefresh, isWin, setWinTime, isLose }) => {\n  let [time, setTime] = useState(0);\n\n  const intervalID = useRef(0);\n\n  const startInterval = () => {\n    intervalID.current = setInterval(() => {\n      time++\n      setTime(time)\n      if (time === 999) {\n        clearInterval(intervalID.current)\n      }\n    }, 1000);\n  }\n\n  const stopInterval = () => {\n    clearInterval(intervalID.current);\n    setTime(0)\n  }\n\n  useEffect(() => {\n    return () => stopInterval();\n  }, []);\n\n  useEffect(() => {\n    if (isRefresh) {\n      stopInterval();\n    } else {\n      startInterval()\n    }\n  }, [isRefresh]);\n\n  useEffect(() => {\n    if (isWin) {\n      clearInterval(intervalID.current);\n      setWinTime(time)\n    }\n    if (isLose) {\n      clearInterval(intervalID.current);\n    }\n  }, [isWin, isLose]);\n\n  return (\n    <>\n      <FiClock style={{ marginLeft: 30 }} />\n      <span style={{ fontSize: 20, margin: 5 }}>\n        {(\"000\" + time).slice(-3)}\n      </span>\n    </>\n  )\n}\n\nexport default Timer;","/home/zuzanna/Pulpit/saper/src/components/modal/Modal.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/modal/Modal.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/index.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/gameParameters/reducers.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/gameParameters/types.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/gameTheme/reducers.tsx",[],"/home/zuzanna/Pulpit/saper/src/reducers/gameTheme/types.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/winModal/WinModal.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/winModal/WinModal.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/pages/MainPage.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/pages/Score.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/score/SingleScore.tsx",[],"/home/zuzanna/Pulpit/saper/src/score/SingleScore.css.tsx",[],"/home/zuzanna/Pulpit/saper/src/components/settings/Settings.tsx",["165","166"],"import React, { useState } from 'react'\nimport { GearButton, SubmitButton, Wrapper } from './Settings.css'\nimport Modal from '../modal/Modal'\nimport Level from './Level'\nimport { Levels } from '../../../reducers/gameParameters/types'\nimport { SetParametersAcition } from '../../../reducers/gameParameters/actions'\nimport { RootState } from '../../../reducers/index'\nimport { useSelector, useDispatch } from 'react-redux'\n\nconst Settings = () => {\n  const selectorParamaters = useSelector((state: RootState) => state.gameParameters)\n\n  const dispatch = useDispatch();\n  const [parameters, setParameters] = useState(selectorParamaters)\n  const [isOpen, setIsOpen] = useState(false);\n\n  const handleChangeParameters = (event: React.ChangeEvent<HTMLInputElement>) => {\n    const { name, value } = event.targe